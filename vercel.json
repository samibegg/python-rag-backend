{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb", 
        "runtime": "python3.12"  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key_secret",
    "HUGGING_FACE_HUB_TOKEN": "@hf_token_secret"
  }
}

